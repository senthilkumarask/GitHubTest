<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE gsa-template
        PUBLIC "-//Art Technology Group, Inc.//DTD General SQL Adapter//EN"
        "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template  xml-combine="append">
    <header>
        <name>Store Template</name>
        <author></author> 
    </header>
    
    <item-descriptor name="store"  xml-combine="append" cache-mode="distributedJMS" item-cache-size="2000" query-cache-size="2000">
    	<table name="BBB_STORE_BOPUS" type="multi" id-column-name="STORE_ID" multi-column-name="SITE_ID">
			<property name="bopus" column-name="BOPUS_FLAG" data-type="map" component-data-type="boolean" display-name-resource="bopus"/>
		</table>
    </item-descriptor>
    
    <item-descriptor name="specialityCode"  xml-combine="append" cache-mode="distributedJMS" item-cache-size="2000" query-cache-size="2000">
	</item-descriptor>
	
	<item-descriptor name="specialityCodeMap" xml-combine="append" cache-mode="distributedJMS">
	</item-descriptor>
</gsa-template>