<gsa-template xml-combine="append">

	<header>
		<name>BBB Custom Catalog</name>
	    <author></author>	
		<version>$Revision: #1 $</version>
	</header>


	<!--/////////////////////////////// -->
	<!-- category -->
	<!--/////////////////////////////// -->
	<item-descriptor name="category" xml-combine="append">
	<table name="BBB_CATEGORY" type="auxiliary" id-column-name="category_id">
		<!-- GS related product attributes starts -->
			<property name="gsImageOrientation" column-name="GS_Image_Orientation" display-name-resource="gsImageOrientation" data-type="enumerated" category-resource="GSCategoryCustom">
				<attribute name="resourceBundle" value="com.bbb.commerce.catalog.BBBGSCatalogTemplateResources" />
				<attribute name="propertySortPriority" value="9" />
				<attribute name="uiwritable" value="true" />
				<attribute name="useCodeForValue" value="true" />
				<option value="overlay" code="0" />
				<option value="nonOverlay" code="1" />
			</property>
		<!-- GS related product attributes ends -->
		</table>
	</item-descriptor>





