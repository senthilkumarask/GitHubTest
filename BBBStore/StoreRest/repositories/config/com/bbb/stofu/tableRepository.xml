		<property name="slave2CatPanel" column-name="catPanel3" display-name-resource="slave2CatPanel" item-type="CategoryPanel" category-resource="MasterInfo" repository="/com/bbb/stofu/CommonRepository">
			<attribute name="resourceBundle" value="com.bbb.stofu.StofuTemplateResources"/>
        		<attribute name="propertySortPriority" value="5"/>
		</property>
		<property name="masterCatPanelSize" column-name="catPanel1Size" display-name-resource="masterCatPanelSize" data-type="string" category-resource="MasterInfo" writable="false" default="?$TBD$">
			<attribute name="resourceBundle" value="com.bbb.stofu.StofuTemplateResources"/>
        		<attribute name="propertySortPriority" value="3"/>
		</property>
		<property name="slave1CatPanelSize" column-name="catPanel2Size" display-name-resource="slave1CatPanelSize" data-type="string" category-resource="MasterInfo" writable="false" default="?$TBD$">
			<attribute name="resourceBundle" value="com.bbb.stofu.StofuTemplateResources"/>
        		<attribute name="propertySortPriority" value="4"/>
		</property>
		<property name="slave2CatPanelSize" column-name="catPanel3Size" display-name-resource="slave2CatPanelSize" data-type="string" category-resource="MasterInfo" writable="false" default="?$TBD$">
			<attribute name="resourceBundle" value="com.bbb.stofu.StofuTemplateResources"/>
        		<attribute name="propertySortPriority" value="5"/>
		</property>
	</table>
</item-descriptor>

