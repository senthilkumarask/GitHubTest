			</test>
		</group>

		<group id="testBuildContextOnCountryCodeError">
			<test id="test2">
				<objs>
					<obj id="internationalShippingBuilder"
						type="com.bbb.internationalshipping.utils.BBBInternationalShippingBuilder"
						hint="call" method="resolveName"
