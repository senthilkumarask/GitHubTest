	<cluster id="com.bbb.internationalshipping.utils.TestBuildContext">
	
	<!-- TestBuildContextFromIP -->
	
	<group id="testBuildContextFromIPSuccess">
        <test id="test1">
			<objs>
				<obj id="internationalShippingBuilder" type="com.bbb.internationalshipping.utils.BBBInternationalShippingBuilderImpl" hint="call" 
				method="resolveName" calltype="com.bbb.internationalshipping.utils.TestBuildContext">
					<item type="string">/com/bbb/internationalshipping/utils/BBBInternationalShippingBuilder</item>
				</obj>
				<obj id="clientIPAddress" type="string">27.255.128.0</obj>
			</objs>      
        </test>
      </group>
      
       <group id="testBuildContextFromIPError">
       <test id="test1">
			<objs>
				<obj id="internationalShippingBuilder" type="com.bbb.internationalshipping.utils.BBBInternationalShippingBuilderImpl" hint="call" 
				method="resolveName" calltype="com.bbb.internationalshipping.utils.TestBuildContext">
					<item type="string">/com/bbb/internationalshipping/utils/BBBInternationalShippingBuilder</item>
				</obj>
				<obj id="clientIPAddress" type="string"></obj>
			</objs>      
        </test>
    </group>
    
    <!-- TestBuildContextOnCountryCode -->
    
   <group id="testBuildContextOnCountryCodeSuccess">
