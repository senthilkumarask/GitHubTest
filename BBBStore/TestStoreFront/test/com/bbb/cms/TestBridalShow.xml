<ddtunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ddtunit.sourceforge.net/ddtunit.xsd">
    <cluster id="com.bbb.cms.TestBridalShow">
      <group id="testBridalShowState">
        <test id="testService_test1">
			<objs>
				<obj id="bridalState" type="com.bbb.cms.droplet.BridalShowStateDroplet" hint="call" method="resolveName" calltype="com.bbb.cms.TestBridalShow">
					<item type="string">/com/bbb/cms/droplet/BridalShowStateDroplet</item>
				</obj>
				<obj id="siteId" type="string">BuyBuyBaby</obj>	
				<obj id="stateId" type="string">AL</obj>
			</objs>      
			
        </test>
      </group>
      <group id="testBridalTemplate">
        <test id="testService_test2">
			<objs>
				<obj id="bridalTemplate" type="com.bbb.cms.droplet.BridalShowDetailDroplet" hint="call" method="resolveName" calltype="com.bbb.cms.TestBridalShow">
					<item type="string">/com/bbb/cms/droplet/BridalShowDetailDroplet</item>
				</obj>
				<obj id="siteId" type="string">BuyBuyBaby</obj>	
				<obj id="stateId" type="string">AL</obj>
			</objs>      
			<asserts>
	        	<assert id="name" type="java.lang.String" action="isEqual">Bridal Show</assert>
	        </asserts>
        </test>
      </group>
    </cluster>
</ddtunit>