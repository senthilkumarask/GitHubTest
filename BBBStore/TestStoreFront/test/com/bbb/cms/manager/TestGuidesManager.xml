<ddtunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ddtunit.sourceforge.net/ddtunit.xsd">
	<cluster id="com.bbb.cms.manager.TestGuidesManager">
		<group id="testGuidesService">
			 
			<test id="testGuidesService_test2">
				<objs>
					<obj id="manager" type="com.bbb.cms.manager.GuidesTemplateManager"
						hint="call" method="resolveName" calltype="com.bbb.cms.manager.TestGuidesManager">
						<item type="string">/com/bbb/cms/manager/GuidesTemplateManager</item>
					</obj>
					<obj id="contentType" type="string">guides</obj>
					<obj id="guidesCategory" type="string">bridal guide</obj>
					<obj id="siteId" type="string">BedBathUS</obj>
				</objs>
				<asserts>
					<assert id="imageAltText" type="java.lang.String" action="isNotNull"></assert>
					<assert id="title" type="java.lang.String" action="isNotNull"></assert> 
					<assert id="lstGuidesSize" type="java.lang.Integer" action="isGT">0</assert>
				</asserts>
			</test>
		</group>
		<group id="testGuidesLongDescService">
			 
			<test id="testGuidesService_test3">
				<objs>
					<obj id="manager" type="com.bbb.cms.manager.GuidesTemplateManager"
						hint="call" method="resolveName" calltype="com.bbb.cms.manager.TestGuidesManager">
						<item type="string">/com/bbb/cms/manager/GuidesTemplateManager</item>
					</obj>
					<obj id="guideId" type="string">100002</obj>
				 </objs>
				<asserts>
					<assert id="title" type="java.lang.String" action="isEqual">How to choose a baby stroller</assert>
					<assert id="longDescription" type="java.lang.String" action="isNotNull"></assert> 
				</asserts>
			</test>
		</group>
		
	</cluster>
</ddtunit>


 