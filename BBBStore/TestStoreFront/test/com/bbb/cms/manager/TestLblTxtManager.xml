					<obj id="siteId" type="string">BuyBuyBaby</obj>
					<obj id="placeHolder" type="java.util.HashMap" hint="map">

					</obj>
				</objs>
				<asserts>
					<assert id="textArea" type="java.lang.String" action="isNotNull"></assert>
				</asserts>
			</test>
		</group>
		<group id="testErrorService">
