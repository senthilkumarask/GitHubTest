<ddtunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ddtunit.sourceforge.net/ddtunit.xsd">
    <cluster id="com.bbb.selfservice.droplet.TestClickToChatDroplet">
      <group id="testService">
        <test id="testService_test1">
			<objs>
				<obj id="clickToChatDroplet" type="com.bbb.selfservice.droplet.ClickToChatDroplet" hint="call" method="resolveName" calltype="com.bbb.selfservice.droplet.TestClickToChatDroplet">
					<item type="string">/com/bbb/selfservice/ClickToChatDroplet</item>
				</obj>
				<obj id="siteId" type="string">BuyBuyBaby</obj>	
		</objs>      
			<asserts> 
	        	<assert id="chatURL" type="java.lang.String" action="isEqual">http://bedbathandbeyond.custhelp.com/app/chat/chat_landing</assert>
	        	<assert id="onOffFlag" type="java.lang.String" action="isEqual">true</assert>
	        	<assert id="weekDayOpenTime" type="java.lang.String" action="isEqual">09:00</assert>
	        	<assert id="weekDayCloseTime" type="java.lang.String" action="isEqual">21:00</assert>
	        	<assert id="weekEndOpenTime" type="java.lang.String" action="isEqual">09:00</assert>
	        	<assert id="weekEndCloseTime" type="java.lang.String" action="isEqual">21:00</assert>
	        </asserts>  
        </test>
      </group>
    </cluster>
</ddtunit>