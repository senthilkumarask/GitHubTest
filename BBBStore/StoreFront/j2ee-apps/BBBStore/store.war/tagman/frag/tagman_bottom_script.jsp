<dsp:page>
	<dsp:importbean bean="/atg/dynamo/droplet/Switch"/>
	<dsp:droplet name="Switch">
		<dsp:param name="value" param="PageType"/>		
		<dsp:oparam name="default">			 
			<%-- Begin TagMan Script Execution Code --%>			 
			<script type="text/javascript">	 
				(function(d,s){ 
					function loadLevexisJS() {
						var client = 'bedbathbeyond'; 
						var siteId = 1; 
						// do not edit 
						var a=d.createElement(s),b=d.getElementsByTagName(s)[0]; 
						a.async=true;a.type='text/javascript'; 
						a.src='//sec.levexis.com/clients/'+client+'/'+siteId+'.js'; 
						a.tagman='st='+(+new Date())+'&c='+client+'&sid='+siteId; 
						b.parentNode.insertBefore(a,b);	
					} 
					
					if (window.attachEvent) {
				        window.attachEvent('onload', loadLevexisJS);
				    }
				    else {
				        window.addEventListener('load', loadLevexisJS, false);
				    }
				})
				(document,'script'); 
			</script> 
			<%-- End TagMan Script Execution Code --%>
		</dsp:oparam>
	</dsp:droplet>
</dsp:page>