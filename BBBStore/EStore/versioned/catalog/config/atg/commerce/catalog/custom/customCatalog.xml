        </property>
  	</table>
 </item-descriptor>
  
  <item-descriptor xml-combine="append" name="bbbskuAttrReln" cache-mode="simple" display-property="id" display-name-resource="SkuAttrReln" last-modified-property="lastModifiedDate">
  	
  	<table name="BBB_SKU_ATTR_RELN" type="auxiliary" id-column-names="sku_attr_reln_id" xml-combine="remove">
  	 	<property name="sku" column-names="sku_id" item-type="sku" writable="false" hidden="true" queryable="true">
        </property>
  	</table>
  	<table name="BBB_PRD_ATTR_RELN" type="auxiliary" id-column-names="prd_attr_reln_id" xml-combine="remove">
  	 	<property name="product" column-names="product_id" item-type="product" writable="false" hidden="true" queryable="true">
        </property>
  	 </table>  	
  
  </item-descriptor>
  
   
  
  
  
  <item-descriptor name="attrTransaltion" xml-combine="append" cache-mode="simple" display-property="id" display-name-resource="SKUAttrTranslation" last-modified-property="lastModifiedDate">
  	
  	<table name="BBB_SKU_ATR_SITE_TRNSLN" type="auxiliary" id-column-names="translation_id" xml-combine="remove">
  	 	<property name="sku" column-names="sku_attribute_id" item-type="sku"  writable="false" hidden="true" queryable="true">
        </property>
  	</table>
  </item-descriptor>
  <item-descriptor name="bbbEcoFeeSKU" cache-mode="simple" xml-combine="append">
  	
  <table name="BBB_SKU_ECO_FEE" type="primary" id-column-name="eco_Fee_reln_id" xml-combine="append">
  		<property name="ecoFeeSKU" column-name="eco_fee_sku_id" display-name-resource="ecoFeeSKU" data-type="string" xml-combine="replace" category-resource="BBBEcoFeeSku">
    		<attribute name="resourceBundle" value="com.bbb.commerce.catalog.BBBCatalogTemplateResources"/>
        	<attribute name="propertySortPriority" value="2"/>
    	</property>  
  
  </table>
  </item-descriptor>
  
  <item-descriptor name="sku" display-property="displayNameDefault" xml-combine="append" last-modified-property="lastModifiedDate">
   <table name="BBB_SKU_COMPUTED_ATTRIBUTES" type="auxiliary" id-column-name="sku_id" xml-combine="remove">
                <attribute name="tablePrefix" value="BBB_MISC."/>
                <attribute name="metaDataSchemaPattern" value="BBB_MISC"/>
        <property name="categoriesList" column-name="list_categories"  data-type="string" />
        <property name="packageCount" column-names="package_cnt"  data-type="int"/>
  </table>
</item-descriptor>
  
  
  
    <item-descriptor name="product" display-property="displayNameDefault" xml-combine="append" last-modified-property="lastModifiedDate">
   <table name="BBB_PROD_COMPUTED_ATTRIBUTES" type="auxiliary" id-column-name="product_id" xml-combine="remove">
                <attribute name="tablePrefix" value="BBB_MISC."/>
                <attribute name="metaDataSchemaPattern" value="BBB_MISC"/>
      	 <property name="categoriesList" column-name="list_categories"  data-type="string" />
  </table>
</item-descriptor>
  
 
</gsa-template>
