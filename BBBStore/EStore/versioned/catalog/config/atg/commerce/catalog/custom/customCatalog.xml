
<!--startSQLRepository -m BBBStore.core -repository /atg/commerce/catalog/ProductCatalog -outputSQLFile D:\Nov29_1.sql-->

<gsa-template xml-combine="append">

  <header>
    <name>BBB Custom Catalog</name>
    <author></author>
    <version>$Revision: #1 $</version>
  </header>
 

 
 <item-descriptor xml-combine="append" name="bbbPrdTranslation" cache-mode="simple" display-property="id" display-name-resource="BBBProductTranslation" last-modified-property="lastModifiedDate">
 	<table name="BBB_PROD_SITE_TRANSLATIONS" type="auxiliary" id-column-names="translation_id" xml-combine="remove">
  		<property name="product" column-names="product_id" item-type="product"  writable="false" queryable="true" hidden="true">
        </property>
  	</table>
 </item-descriptor>
  
  
  
   
   <item-descriptor xml-combine="append" name="bbbOtherMedia" cache-mode="simple" display-property="id" display-name-resource="ProdMedia" last-modified-property="lastModifiedDate">
   
  		<table name="BBB_PRD_OTHER_MEDIA" type="auxiliary" id-column-names="media_id" xml-combine="remove">
	  		<property name="product" column-names="product_id" item-type="product" writable="false" queryable="true" hidden="true">
	        </property>
	  	</table>	
   </item-descriptor>
  
  
  
  <item-descriptor xml-combine="append" name="bbbSKUTranslation" cache-mode="simple" display-property="id"  display-name-resource="SkuTranslation" last-modified-property="lastModifiedDate">
  	<table name="BBB_SKU_SITE_TRANSLATIONS" type="auxiliary" id-column-names="translation_id" xml-combine="remove">
