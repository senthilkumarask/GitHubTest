<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE gsa-template
        PUBLIC "-//Art Technology Group, Inc.//DTD General SQL Adapter//EN"
        "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
    <header>
        <name>BBB Sales Feed Repository Template</name>
        <author></author>
    </header>
     <item-descriptor name="feedSchedularRepository" display-name="feedSchedularRepository" display-property="feedTypeDesc" item-cache-size="8000" cache-mode="simple" query-cache-size="8000" >
    	<table name="BBB_FEED_SCHEDULER" type="primary" id-column-name="feed_key_id">
			<property name="id" column-name="feed_key_id" display-name="id" writable="true">
				<attribute name="propertySortPriority" value="0" />	
			</property>
			<property name="feedTypeDesc" column-name="feed_type_desc" display-name="feedTypeDesc" data-type="string">
				<attribute name="propertySortPriority" value="2" />	
			</property>
			<property name="feedType" data-type="enumerated" column-name="feed_type">
				<attribute name="useCodeForValue" value="false"/>
				<attribute name="propertySortPriority" value="1" />	
				<option value="salesDataFeed" code="10001"/>				
			</property>
		</table>
		<table name="BBB_FEED_SCHEDULER_KEYS" type="multi" id-column-name="feed_key_id" multi-column-name="feed_key">
			<property name="feedSchedulerValue" column-name="feed_scheduler_value" display-name="feedSchedulerValue" data-type="map" component-data-type="string" >
				<attribute name="propertySortPriority" value="3" />
			</property>
		</table>
        <named-query>
			<rql-query>
				<query-name>getFeedValueByFeedType</query-name>
				<rql>feedType=?0</rql>
			</rql-query>
		</named-query>
	</item-descriptor>
	<query-items item-descriptor="feedSchedularRepository" quiet="true">ALL</query-items>
</gsa-template>

