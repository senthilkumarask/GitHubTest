	<cluster id="com.bbb.rest.email.manager.TestBBBGSEmailSender">
			<group id="testSendFeedbackMail">
			<test id="test1">
				<objs>
					<obj id="atg-rest-depth" type="string">0</obj>
					<obj id="atg-rest-input" type="string">json</obj>
