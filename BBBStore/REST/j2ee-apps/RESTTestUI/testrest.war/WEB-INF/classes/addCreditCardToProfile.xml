		<param id="billAddrValue.country"  name="country" class="reqBody" type="text"></param>
		<param id="billAddrValue.postalCode" name="postalCode" class="reqBody" type="text"></param>
		<param id="makePreferredSet"  name="makePreferredSet" class="reqBody" type="text">true</param>
		<param type="text" name="clientID" id="clientID" required="true" class="reqHead" >client123</param>
		<param type="text"  name="token" id="token" class="reqHead" >client123</param>
		<param type="select" name="X-bbb-site-id" id="X-bbb-site-id" class="reqHead">BuyBuyBaby,BedBathUS,BedBathCanada</param>
