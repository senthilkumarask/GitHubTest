		<class-b>com.bedbathandbeyond.www.ValidateOldAccountDocument$ValidateOldAccount</class-b>
		<field>
			<a>userToken</a>
			<b>userToken</b>
		</field>
		<field>
			<a>siteFlag</a>
			<b>siteFlag</b>
		</field>
		<field>
			<a>email</a>
			<b>email</b>
		</field>
		<field>
			<a>password</a>
			<b>password</b>
		</field>
	</mapping>

	<mapping>
