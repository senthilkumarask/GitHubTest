                  </div>
                  <!-- /.progress-group -->
                  <div class="progress-group">
                    <span class="progress-text">TBS Orders</span>
                    <span class="progress-number"><b>${submittedOrderCountTBS}</span>

                    <div class="progress sm">
                      <div class="progress-bar progress-bar-yellow" style="width: 100%"></div>
                    </div>
                  </div>
                  <!-- /.progress-group -->
                </div>
                <!-- /.col -->
              </div>
              <!-- /.row -->
            </div>
            <!-- ./box-body -->
            
            <!-- /.box-footer -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->






