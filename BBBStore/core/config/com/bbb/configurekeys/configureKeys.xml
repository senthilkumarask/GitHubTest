				<attribute name="propertySortPriority" value="1" />	
			</property>
			
		<property name="configKey" column-name="config_key" display-name-resource="configKeyDefault" data-type="string" category-resource="BasicInfo">
		<attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources"/>
				<attribute name="propertySortPriority" value="2" />	
		</property>
				
		<property name="configValueDefault" column-name="config_value" display-name-resource="configValueDefault" data-type="string" category-resource="BasicInfo">
		<attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources"/>
				<attribute name="propertySortPriority" value="3" />
		</property>
		</table>
		
		 <property name="configValue" display-name-resource="configValue" data-type="string" hidden="true">
           <derivation user-method="com.bbb.framework.cms.ConfigKeySiteChannelTranslation">
                  <expression>configValueDefault</expression>       
           </derivation>
           <attribute name="propertySortPriority" value="2"/> 
           <attribute name="defaultProperty" value="configValueDefault"/>
           <attribute name="defaultSite" value="BedBathUS"/>
           <attribute name="defaultChannel" value="DesktopWeb" />
           <attribute name="property" value="configValue"/>
           <attribute name="translations" value="translations"/>
       </property>
      
		
