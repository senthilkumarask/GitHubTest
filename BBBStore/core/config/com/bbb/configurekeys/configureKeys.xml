             <attribute name="propertySortPriority" value="1"/> 
       </property>
       <property name="channel" column-name="CHANNEL_ID" display-name-resource="Channel" item-type="channelInfo" repository="/com/bbb/channel/ChannelRepository" category-resource="TranslationInfo">
              <attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources"/>
              <attribute name="propertySortPriority" value="2"/> 
       </property>
       <property name="configValue" column-name="config_value" display-name-resource="configValue" data-type="string" category-resource="TranslationInfo">
         <attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources"/>
          <attribute name="propertySortPriority" value="3"/>    
