        		<attribute name="propertySortPriority" value="0" />
       </property>
       <property name="site" column-name="SITE_ID" display-name-resource="translationSite" item-type="siteConfiguration" repository="/atg/multisite/SiteRepository" category-resource="TranslationInfo">
