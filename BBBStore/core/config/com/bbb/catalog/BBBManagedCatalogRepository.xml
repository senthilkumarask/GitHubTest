	        	<attribute name="propertySortPriority" value="2"/>
    		</property>
    		<property name="showFilters" column-name="SHOW_FILTERS" display-name-resource="showFilters" data-type="boolean" default="true">
	    		<attribute name="resourceBundle" value="com.bbb.commerce.catalog.BBBCatalogTemplateResources"/>
	        	<attribute name="propertySortPriority" value="2"/>
    		</property>
    	</table>		
		
		<table name="BBB_PROMO_PROD_ATTRIBUTES" type="multi" id-column-names="category_id"	multi-column-name="seq_number"  shared-table-sequence="1">
