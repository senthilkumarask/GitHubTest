    <item-descriptor name="bccManagedPromotionCategory" sub-type-value="bccManagedPromotionCategory" super-type="bccManagedCategory" display-name="BCC Managed Promotional Category" cache-mode="simple" item-cache-size="10000" query-cache-size="10000" display-property="categoryName" last-modified-property="lastModifiedDate">
