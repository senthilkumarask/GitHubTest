				<attribute name="resourceBundle" value="com.bbb.commerce.catalog.BBBCatalogTemplateResources"/>
        		<attribute name="propertySortPriority" value="0"/>
			</property>
			<property name="lastModifiedDate" column-name="last_mod_date" display-name-resource="lastModifiedDate" data-type="timestamp" hidden="true">
  				<attribute name="resourceBundle" value="com.bbb.commerce.catalog.BBBCatalogTemplateResources"/>
        		<attribute name="propertySortPriority" value="1"/>
  			</property>
  			<property name="sortingOptionNameDefault" column-name="sort_opt_name_default" data-type="string" hidden="true">
  				<attribute name="resourceBundle" value="com.bbb.commerce.catalog.BBBCatalogTemplateResources"/>
        		<attribute name="propertySortPriority" value="2"/>
  			</property>
   		</table>
	  	<table name="BBB_SORT_OPTION_LIST" type="multi" id-column-names="id" multi-column-name="sequence_num">
	 		<property name="sortingOptions" column-names="SORTING_OPTION_ID" display-name-resource="sortingOptions" data-type="list" component-item-type="sortingOption" category-resource="BBBSortingOption" repository="/com/bbb/catalog/BBBManagedCatalogRepository">
