			<derivation user-method="com.bbb.commerce.catalog.GenericListTranslation">
            	<expression>sortingOptionNameDefault</expression>
			</derivation>
			<attribute name="property" value="sortingOptions"/>
			<attribute name="itemDescProperty" value="sortingValue"/>
	     </property>
 	</item-descriptor>
