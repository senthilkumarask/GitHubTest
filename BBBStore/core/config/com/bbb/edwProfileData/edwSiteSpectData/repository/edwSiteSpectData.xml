		 </property>
		 
		<property name="sendToSiteSpect" column-name="SEND_TO_SITESPECT" display-name-resource="requiredInputSiteSpect" data-type="boolean" category-resource="EDW SiteSPect Data Input">
			<attribute name="resourceBundle" value="com.bbb.edwData.BBBEDWProfileDataResources"/>
        		<attribute name="propertySortPriority" value="2"/>
		</property>
		
		<property name="sendToTIBCO" column-name="SEND_TO_TIBCO" display-name-resource="requiredInputTIBCO" data-type="boolean" category-resource="EDW SiteSPect Data Input">
			<attribute name="resourceBundle" value="com.bbb.edwData.BBBEDWProfileDataResources"/>
        		<attribute name="propertySortPriority" value="3"/>
		</property>
		</table>
		
	</item-descriptor>
