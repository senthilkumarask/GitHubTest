<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE gsa-template
        PUBLIC "-//Art Technology Group, Inc.//DTD General SQL Adapter//EN"
        "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
    <header>
        <name>RegistrantLanding Template</name>
        <author></author>
    </header>
     
  <!--///////////////////////////////-->
  <!--          registrantLanding             -->
  <!--///////////////////////////////-->
  
<item-descriptor name="registrantLanding" display-name-resource="registrantLanding" display-property="pageTitle" cache-mode="simple" item-cache-size="100" query-cache-size="100">
	<attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources"/>
	
	<table name="BBB_REGISTRANT_TEMPLATE" type="primary" id-column-name="registrant_template_id">
		<property name="id" column-name="registrant_template_id" display-name-resource="registrantTemplateId" category-resource="BasicInfo" writable="true">
        		<attribute name="propertySortPriority" value="0" />
             	<attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources" />
        </property>
        <property name="site" column-name="site_id" display-name-resource="siteId" item-type="siteConfiguration" category-resource="BasicInfo" 
        		repository="/atg/multisite/SiteRepository">
            <attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources"/>
            <attribute name="propertySortPriority" value="1"/>
        </property>  
       <property name="registryType" column-name="registry_type" display-name-resource="registryType" item-type="registrytype" repository="/atg/multisite/SiteRepository" category-resource="BasicInfo">
       		<attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources" />
       		<attribute name="propertySortPriority" value="2"/>
       </property>	
       <property name="channel" column-name="channel" item-type="channelInfo" repository="/com/bbb/channel/ChannelRepository" display-name-resource="channel" category-resource="BasicInfo">
			<attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources" />
			<attribute name="propertySortPriority" value="3"/>
		</property> 
			<property name="pageTitle" column-name="PAGE_TITLE" display-name-resource="pageTitle" data-type="string" category-resource="commonInfo">
            <attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources"/>
            <attribute name="propertySortPriority" value="4"/>
           </property>
	</table>
	
    <table name="BBB_REGISTRANT_PBOX_TOP" type="auxiliary"  id-column-name="registrant_template_id" >
     <property name="upperPromoBoxSlot" column-name="upperPromoBoxSlot_id" display-name-resource="upperPromoBoxSlot" item-type="promoBox" category-resource="BasicInfo" 
        		repository="/com/bbb/cms/repository/LandingTemplate">
          <attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources"/>
          <attribute name="propertySortPriority" value="4"/>
        </property>   
    </table>  
     
    <table name="BBB_REGISTRANT_PBOX_MIDDLE" type="multi" multi-column-name="sequence_num" id-column-name="registrant_template_id" >
      <property name="middlePromoBoxSlot" column-names="middlePromoBoxSlot_id"  data-type="list" display-name-resource="middlePromoBoxSlot" component-item-type="promoBox" 
       		category-resource="BasicInfo" repository="/com/bbb/cms/repository/LandingTemplate">
       <attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources"/>
       <attribute name="propertySortPriority" value="5"/>
      </property>
    </table>
    
     <table name="BBB_REGISTRANT_PBOX_BOTTOM" type="auxiliary"  id-column-name="registrant_template_id" >
     <property name="bottomPromoBoxSlot" column-name="bottomPromoBoxSlot_id" display-name-resource="bottomPromoBoxSlot" item-type="promoBox" category-resource="BasicInfo" 
        		repository="/com/bbb/cms/repository/LandingTemplate">
          <attribute name="resourceBundle" value="com.bbb.cms.CmsTemplateResources"/>
          <attribute name="propertySortPriority" value="6"/>
        </property>   
    </table>  
</item-descriptor>
</gsa-template>