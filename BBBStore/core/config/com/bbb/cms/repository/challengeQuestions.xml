<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE gsa-template
        PUBLIC "-//Art Technology Group, Inc.//DTD General SQL Adapter//EN"
        "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
    <header>
        <name>Challenge Questions</name>
        <author></author>
    </header>
	    <item-descriptor name="questions" cache-mode="simple" item-cache-size="10000"  query-cache-size="1000" display-property="challengeQuestions">
	      <table name="BBB_CHALLENGE_QUESTIONS" type="primary" id-column-name="CHALLENGE_QUESTION_ID">
		       <property name="questionType" data-type="enumerated" column-name="QUESTION_TYPE"  display-name="questionType" >
					<attribute name="useCodeForValue" value="false" />
					<option value="Question1" code="1" />
					<option value="Question2" code="2" />
		      </property>
		      <property name="challengeQuestions" data-type="string" column-name="CHALLENGE_QUESTION" display-name="challengeQuestions"/>
	      </table>
	    </item-descriptor>	
</gsa-template>