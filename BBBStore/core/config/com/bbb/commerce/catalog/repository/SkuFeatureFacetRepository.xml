			</property>
		</table>
	</item-descriptor>
	
<item-descriptor name="dimOrder" cache-mode="simple" item-cache-timeout="5000" writable="true" query-expire-timeout="5000">
   <table name="BBB_DIM_ORDER" type="primary" id-column-name="CATEGORY_ID">
       <property name="categoryId" column-names="CATEGORY_ID" data-type="string" readable="true" writable="true" queryable="true"/>
       <property name="rank" column-name="rank" data-type="string" readable="true" writable="true" queryable="true"/>
   </table>
</item-descriptor>
</gsa-template>
