	<!--/////////////////////////////// -->
	<!-- skuFacetRepository -->
	<!--/////////////////////////////// -->


	<item-descriptor expert="false" display-name="skuFacetRepository" id-separator=":" writable="true"  query-cache-size="1000"   name="skuFacet"    item-cache-size="1000">


		<table id-column-names="FACET_ID,FACET_VALUE_ID" name="BBB_FACET_FACET_VALUE_TMP" type="primary" shared-table-sequence="1">
			
			      <!-- <property name="id" column-names="ITEM_ID,FACET_ID" display-name="id" 
				writable="true"> </property> -->
		 
			<property expert="false" readable="true" category="MasterInfo" data-type="big string" column-name="FACET_ID" display-name="facetType" hidden="false" writable="true" name="facetType" required="false" cache-mode="inherit" queryable="true">
			 
			</property>
			<property expert="false" readable="true" category="MasterInfo" column-name="FACET_VALUE" display-name="facetValue" data-type="big string" hidden="false" writable="true" name="facetValue" required="false" cache-mode="inherit" queryable="true">
		 
			</property>
	 		<property expert="false" readable="true" category="MasterInfo" column-name="DESCRIPTION" display-name="description" data-type="big string" hidden="false" writable="true" name="description" required="false" cache-mode="inherit" queryable="true">
			</property>
