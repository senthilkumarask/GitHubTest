		<property name="cutOffTime" column-name="cut_off_time" display-name-resource="cutOffTime" data-type="timestamp" category-resource="MasterInfo">
			<attribute name="resourceBundle" value="com.bbb.commerce.shipping.BBBShippingTemplateResources"/>
