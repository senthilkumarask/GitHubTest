<?xml version="1.0" encoding="UTF-8"?>
<!--Created with Liquid XML 2014 Designer Edition 12.0.3.4958 (http://www.liquid-technologies.com)-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="unqualified">
   <xs:element name="orders">
      <xs:annotation>
         <xs:documentation>Multiple orders will be sent in one message.</xs:documentation>
         <xs:documentation>Version - 20160525</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element name="order" type="OrderType" maxOccurs="2" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:simpleType name="OrderTypes">
      <xs:restriction base="xs:string">
         <xs:enumeration value="DELIVERY" />
         <xs:enumeration value="BOPUS" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="PaymentTypes">
      <xs:restriction base="xs:string">
         <xs:enumeration value="CREDIT_CARD" />
         <xs:enumeration value="GIFT_CARD" />
         <xs:enumeration value="PAYPAL" />
         <xs:enumeration value="POS" />
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="OrderType">
      <xs:sequence>
         <xs:element name="orderInfo" type="OrderInfoType">
            <xs:annotation>
               <xs:documentation>This structure defines the header level
						information of an order</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="orderPriceInfo" type="OrderPriceInfoType">
            <xs:annotation>
               <xs:documentation>This structure defines the order level pricing
						information</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="orderTaxInfo" type="TaxInfoType" minOccurs="0">
            <xs:annotation>
               <xs:documentation>This structure defines the tax info of an order</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="billingAddress" type="AddressType">
            <xs:annotation>
               <xs:documentation>Structure for billing address information</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="shipmentList" type="ShipmentListType">
            <xs:annotation>
               <xs:documentation>Structure for the details of shipping group in an order</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="paymentList" type="PaymentListType" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Structure for the details of payment group in an order</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="OrderInfoType">
      <xs:sequence>
         <xs:element name="atgOrderId" type="xs:string">
            <xs:annotation>
               <xs:documentation>OrderId generated in the ATG system.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="orderNumber" type="xs:string">
            <xs:annotation>
               <xs:documentation>BBB Order Number</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="orderType" type="OrderTypes">
            <xs:annotation>
               <xs:documentation>Order Type [ONLINE, BOPUS]</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="orderDate" type="xs:dateTime">
            <xs:annotation>
               <xs:documentation>The order submission date.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="atgProfileId" type="xs:string">
            <xs:annotation>
               <xs:documentation>This is atg profile Id of the user who submitted the order.
						We will pass for all orders, but if user placed an order as guest and didn't register,
						any lookup to ATG system for that profileId will return NO_DATA_FOUND.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="mbrNum" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>OPTIONAL, NOT AVAILABLE for Annonymous and Non Legacy user</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ipAddress" type="xs:string">
            <xs:annotation>
               <xs:documentation>IP address of the user placed the order.
						The IPAddress will be the remote addr from request.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="siteId">
            <xs:annotation>
               <xs:documentation>One of the siteId configured in ATG Site configuration</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="BedBathUS" />
                  <xs:enumeration value="BedBathCanada" />
                  <xs:enumeration value="BuyBuyBaby" />
                  <xs:enumeration value="TBSBedBathUS" />
                  <xs:enumeration value="TBSBedBathBaby" />
                  <xs:enumeration value="TBSBedBathCanada" />
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="currencyCode" minOccurs="0">
            <xs:annotation>
               <xs:documentation>USD for US sites and CAD for .ca site</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="USD" />
                  <xs:enumeration value="CAD" />
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="cartCreate" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>cartCreate is the channel that created the order upon the first add to cart (MOBILE/DESKTOP)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="cartFinal" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>cartFinal is the channel that closed the order status (MOBILE/DESKTOP)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="salesOS" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>sales OS is OS through which order placed</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="schoolPromo" type="SchoolPromoType" minOccurs="0" />
         <xs:element name="affiliate" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Affiliate name in case user has placed has order through affiliate referral</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="OrderInfoAttr" type="OrderInfoAttrType" minOccurs="0" />
         <xs:element name="taxExemptId" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>taxExemptId is used for processing BorderFree orders and required for DS to process orders without domestic sales tax</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="emailSignUp" type="xs:boolean" minOccurs="0" default="false">
            <xs:annotation>
               <xs:documentation>This structure defines if the customer requested for email sign up during checkout</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="tbsStoreNo" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>TBS Next - Store Number of the order</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="SchoolPromoType">
      <xs:sequence>
         <xs:element name="schoolID" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>SchoolID.
						The SchoolID will be from school specific URL.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="promoCode" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>promoCode.
						The promoCode is the coupon code, This coupon contains school promotion.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="OrderPriceInfoType">
      <xs:sequence>
         <xs:element name="orderTotal" type="xs:decimal">
            <xs:annotation>
               <xs:documentation>Total amount of the order (BOPUS + ONLINE).   	
						Includes Items total + Shipping + Surcharge (Item + Delivery)+ GiftWrapCost + Assembly Fee + Tax</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="totalShipping" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Total shipping amount for all shipping groups, will include surcharge also.
						OPTIONAL, For BOPUS ONLY ORDER this will not be available.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="totalSurcharge" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Total surcharge for all shipping groups. OPTIONAL, For BOPUS ONLY ORDER this will not be available.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="totalGiftWrapCost" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Total gift wrap cost for all shipping groups. OPTIONAL, For BOPUS ONLY ORDER this will not be available.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="adjustmentList" type="AdjustmentListType" minOccurs="0">
            <xs:annotation>
               <xs:documentation>All order level adjustments are prorated with items and will be passed with item price,
						this will empty. Keeping it for future purpose.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="totalDeliveryCharge" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>This is the total delivery charge on the order</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="TaxInfoType">
      <xs:sequence>
         <xs:element name="totalTax" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Total tax for the order (Since there is no tax for BOPUS, it will just be ONLINE part).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="cityTax" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>break-up of total tax into city tax.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="countyTax" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>break-up of total tax into county tax</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="districtTax" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>break-up of total tax into district tax.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="stateTax" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>break-up of total tax into state tax</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="AddressType">
      <xs:sequence>
         <xs:element name="firstName" type="xs:string">
            <xs:annotation>
               <xs:documentation>First Name</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="middleName" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>OPTIONAL Middle Name</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="lastName" type="xs:string">
            <xs:annotation>
               <xs:documentation>Last Name</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="companyName" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>OPTIONAL Company Name</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="emailAddress" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Email</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="homePhoneNumber" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Customer Home Phone Number</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="workPhoneNumber" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Customer Work Phone Number</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="addressLine1" type="xs:string">
            <xs:annotation>
               <xs:documentation>Address Line 1</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="addressLine2" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Address Line 2.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="city" type="xs:string">
            <xs:annotation>
               <xs:documentation>City Name</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="state" type="xs:string">
            <xs:annotation>
               <xs:documentation>State (APO/FPO)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="zipCode" type="xs:string">
            <xs:annotation>
               <xs:documentation>Zip, Zip + 4, Postal Code</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="countryCode" type="xs:string">
            <xs:annotation>
               <xs:documentation>Country</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ShipmentListType">
      <xs:sequence>
         <xs:element name="shipmentInfo" type="ShipmentInfoType" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="PaymentListType">
      <xs:sequence>
         <xs:element name="paymentInfo" type="PaymentInfoType" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ItemListType">
      <xs:sequence>
         <xs:element name="item" type="ItemType" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="AdjustmentListType">
      <xs:sequence>
         <xs:element name="adjustment" type="AdjustmentType" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="AdjustmentType">
      <xs:sequence>
         <xs:element name="atgPromotionId" type="xs:string">
            <xs:annotation>
               <xs:documentation>ATG Promotion Id of the promotion applied.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="couponCode" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Coupon Code for this discount.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="discountAmount" type="xs:decimal">
            <xs:annotation>
               <xs:documentation>The discount due the promotion.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="adjustmentDesc" type="xs:string">
            <xs:annotation>
               <xs:documentation>Adjustment description from ATG</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="adjustmentType" type="xs:string">
            <xs:annotation>
               <xs:documentation>Adjustment Type from ATG. e.g. Order Discount - Amount Off</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="itemPriceOverrideReasonCode" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>TBS Next - Holds the reason for the item price override.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="itemPriceOverrideCompetitor" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>TBS Next - Holds the competitor for the item price override.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="itemPriceOverride" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>TBS Next - New price of the item after overriding</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="shippingFeeOverrideReasonCode" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>TBS Next - Holds the reason for the Shipping fee override.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="shippingFeeOverrideCompetitor" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>TBS Next - Holds the competitor for the shipping fee override.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="shipFeeOverride" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>TBS Next - New shipping fee after overriding</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="itemSurchargeOverrideReasonCode" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>TBS Next - Holds the reason for the item surcharge override.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="itemSurchargeOverrideCompetitor" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>TBS Next - Holds the competitor for the item surcharge override.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="itemSurchargeOverride" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>TBS Next - New surcharge of the item after overriding</xs:documentation>
            </xs:annotation>
         </xs:element>
		 <xs:element name="assemblyFeeOverrideReasonCode" type="xs:string" minOccurs="0"> 
            <xs:annotation> 
               <xs:documentation>TBS Next - Holds the reason for the assembly fee override.</xs:documentation> 
            </xs:annotation> 
         </xs:element> 
         <xs:element name="assemblyFeeOverrideCompetitor" type="xs:string" minOccurs="0"> 
            <xs:annotation> 
               <xs:documentation>TBS Next - Holds the competitor for the assembly fee override.</xs:documentation> 
            </xs:annotation> 
         </xs:element> 
         <xs:element name="assemblyFeeOverride" type="xs:decimal" minOccurs="0"> 
            <xs:annotation> 
               <xs:documentation>TBS Next - New assembly fee of the item after overriding</xs:documentation> 
            </xs:annotation> 
         </xs:element> 
	</xs:sequence>
   </xs:complexType>
   <xs:complexType name="ShipmentInfoType">
      <xs:sequence>
         <xs:element name="atgShippingGroupId" type="xs:string">
            <xs:annotation>
               <xs:documentation>Shipping Group Id to uniquely indentify a shipment for order status.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="subTotal" type="xs:decimal">
            <xs:annotation>
               <xs:documentation>Shipping Group Subtotal, sum of all items amount in this group.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:sequence>
            <xs:annotation>
               <xs:documentation>For normal shipping orders, a shipment can be of type to-be-shipped or store pick-up but for SDD Order we will have both Shipping Info and Store Info</xs:documentation>
            </xs:annotation>
            <xs:element name="shippingInfo" type="ShippingInfoType" minOccurs="0"/>
            <xs:element name="storeInfo" type="StoreInfoType"  minOccurs="0" />
         </xs:sequence>
         <xs:element name="itemList" type="ItemListType">
            <xs:annotation>
               <xs:documentation>This structure defines the fields for items in a shipment</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="trackingNumber" type="xs:string" minOccurs="0"> 
           <xs:annotation><xs:documentation>Shipment tracking number to reference delivery status from the 3rd Party system. </xs:documentation> 
           </xs:annotation> 
        </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ShippingInfoType">
      <xs:sequence>
         <xs:element name="shippingMethod" type="xs:string">
            <xs:annotation>
               <xs:documentation>Shipping method [Standard - 3g, Express - 1a, Expedited - 2b, Curbside - LC, Threshold - LT, Room of Choice - LR, WhiteGlove = LW, Same Day Delivery - SDD].</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="shipOnDate" type="xs:date" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Hold the shipment until the given date</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="registryId" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Registry Id if address is registry address.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="shippingAddress" type="AddressType">
            <xs:annotation>
               <xs:documentation>This structure defines the fields for shipping
						address</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="shippingPriceInfo" type="ShippingPriceInfoType">
            <xs:annotation>
               <xs:documentation>This structure defines the fields for price
						details at shipping group level</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice>
            <xs:element name="taxCalculationFailure" type="xs:boolean">
               <xs:annotation>
                  <xs:documentation>Indicates if the tax calculation was a success or failure</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="taxInfo" type="TaxInfoType" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Total tax of this shipping group.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
         <xs:element name="giftInfo" type="GiftInfoType" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Total tax of this shipping group.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="GiftInfoType">
      <xs:sequence>
         <xs:element name="message" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>OPTIONAL gift message.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="charge" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>OPTIONAL Gift Wrap Charge.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="skuId" type="xs:string" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Gift Wrap SkuId.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="giftTaxInfo" type="TaxInfoType" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Total tax of this shipping group.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ShippingPriceInfoType">
      <xs:sequence>
         <xs:element name="shippingRawAmount" type="xs:decimal">
            <xs:annotation>
               <xs:documentation>Shipping amount, which includes surcharge.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="shippingAmount" type="xs:decimal">
            <xs:annotation>
               <xs:documentation>Shipping amount, which includes surcharge.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="surcharge" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Raw total surcharge for this shipping group.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="finalSurcharge" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Final total surcharge for this shipping group.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="shipDeliverySurcharge" type="xs:decimal" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Final total delivery surcharge on the ship group</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="shippingTaxInfo" type="TaxInfoType" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Total tax of this shipping group.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="adjustmentList" type="AdjustmentListType" minOccurs="0">
            <xs:annotation>
               <xs:documentation>This structure defines the fields for the shipping promotion applied. [FREE SHIP OVER $49]</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="StoreInfoType">
      <xs:sequence>
         <xs:element name="storeId" type="xs:string">
            <xs:annotation>
               <xs:documentation>Store Id.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="storeAddress" type="StoreAddressType" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Structure for store address information</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="PaymentInfoType">
      <xs:sequence>
         <xs:element name="paymentId" type="xs:string">
            <xs:annotation>
               <xs:documentation>ATG Payment Group Id.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="paymentType" type="PaymentTypes">
            <xs:annotation>
